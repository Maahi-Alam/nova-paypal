{
    "name": "naif/paypal",
    "description": "A Laravel Nova card to display PayPal current balance",
    "keywords": [
        "laravel",
        "nova",
        "paypal"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Naif\\Paypal\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\Paypal\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
